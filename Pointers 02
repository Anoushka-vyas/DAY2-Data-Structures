#include<bits/stdc++.h>
using namespace std;

int call_by_value(int n){
    cout<<"The address of n in call_by_value function is: "<<&n<<endl;
    n *=n;
    return n;
}
int main(){
    int n=10;
    cout<<"Address of n in main is: "<<&n<<endl;
    cout<<"Ans: "<<call_by_value(n)<<endl;
    cout<<"Value of n is: "<<n<<endl;
    
    return 0;
  
}
